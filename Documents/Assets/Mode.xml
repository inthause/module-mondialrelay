<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.rbs.fr/schema/change-document/4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.rbs.fr/schema/change-document/4.0 http://www.rbschange.fr/schema/4.0/document.xsd"
		extends="Rbs_Shipping_Mode">
	<properties>

		<property name="vendorcode" type="String" required="true"/>
		<property name="vendorprivatekey" type="String" required="true"/>
		<property name="action" type="String"/>
		<property name="delay" type="Integer" default-value="0"/>
		<property name="searchradius" type="Integer" default-value="50"/>
		<property name="addressFields" type="Document" document-type="Rbs_Geo_AddressFields" />

	</properties>
</document>